            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "white-october/pagerfanta-bundle": "^1.0",
        "sidus/base-bundle": "^1.0",
        "doctrine/doctrine-bundle": "*",
        "doctrine/orm": "*"
